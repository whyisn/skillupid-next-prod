# ==== Supabase ====
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE=

# Bucket for certificates (create in Supabase Storage)
SUPABASE_CERT_BUCKET=certificates

# ==== Payments (Midtrans) ====
MIDTRANS_SERVER_KEY=
MIDTRANS_CLIENT_KEY=
MIDTRANS_IS_PRODUCTION=false  # set true in production
# Webhook verification header name depends on Midtrans (VA notifications use signature_key in body)

# ==== Video ====
VIDEO_PROVIDER=mux           # mux | vimeo
# Mux (recommended)
MUX_TOKEN_ID=
MUX_TOKEN_SECRET=
MUX_SIGNING_KEY_ID=
MUX_SIGNING_PRIVATE_KEY=-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----
# Vimeo (alternative)
VIMEO_ACCESS_TOKEN=

# ==== App ====
NEXT_PUBLIC_SITE_URL=https://skillupid.vercel.app
